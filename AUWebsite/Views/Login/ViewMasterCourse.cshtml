
@{
    ViewBag.Title = "ViewMasterCourse";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ViewMasterCourse</h2>

<h4>All Courses</h4>

<div>
    @foreach (var course in ViewBag.Courses)
    {
        <div>
            <h5>Name: @course.Name ID: @course.CourseID Credit Hours: @course.CreditHours Time Start: @course.TimeSlotStart</h5>
            <table class="table table-striped table-hover ">
                <thead>
                    <tr>
                        <th>First Name</th>
                        <th>Last Name</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var person in course.Enrolled)
                    {
                        <tr>
                            <td>@person.FirstName</td>
                            <td>@person.LastName</td>
                        </tr>

                    }
                </tbody>
            </table>
        </div>
    }
</div>

@using (Html.BeginForm("RegistarPage", "Login"))
{
    <button type="submit" class="btn btn-primary" id="WhatisClicked">Return</button>

}

